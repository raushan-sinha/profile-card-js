{
    "manifest_version": 3,
    "name": "Profile Card Extension",
    "version": "1.0",
    "description": "A Chrome extension that displays a stylish profile card with links and details.",
    "action": {
      "default_popup": "index.html",
      "default_title": "Profile Card"
    },
    "permissions": [],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
    }
  }
  